<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow
                          http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
	<!-- start : view path refer to view resolver -->
	<view-state id="personal" view="flows/register/signup-personal"></view-state>

	<view-state id="billing" view="flows/register/signup-billing"></view-state>

	<view-state id="confirm" view="flows/register/signup-confirm"></view-state>

	<!-- end -->
	<end-state id="home2" view="externalRedirect:http://google.com/"></end-state>

	<end-state id="success" view="flows/register/signup-success"></end-state>


	<!-- global transaction -->
	<global-transitions>
		<transition on="home1" to="home2"></transition>
	</global-transitions>
</flow>